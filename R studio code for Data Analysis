################################ 
Data Analysis 6: CUSUM Test of Recursive Residuals for Stability
################################ 

install.packages("strucchange")
library(strucchange)

# Convert to formula format again
formula_ardl <- d_logrgdp ~ L_logrgdp + L_logdsp + L_logfrp + L_loggfcf +
                             lag(d_logrgdp, -1) + lag(d_logdsp, -1) +
                             lag(d_logfrp, -1) + lag(d_loggfcf, -1)

# Recursive residuals
cusum_model <- efp(formula_ardl, data = as.data.frame(na.omit(ecm_data)), type = "Rec-CUSUM")
plot(cusum_model)

